% --- model of a single fuel pebble from the Xe-100 design

% --- for triso particle, need fuel core, buffer, pyro-carbon. and silicon carbide mats

set acelib "/home/amandabachmann/serpent/endfb71_ace/endfb71.xsdata"
set declib "/home/amandabachmann/serpent/endfb71_ace/endfb71.dec"
set nfylib "/home/amandabachmann/serpent/endfb71_ace/endfb71.nfy"

/************************
 * Material definitions *
 ************************/

% --- fuel: UCO , enrichment of 15.5%
% --- density/chemical formula of AGR2 UCO from nagley et. all "fabrication..."

mat fuel      -11.0 rgb 100 50 100 burn 1
92232.81c     -1.7923e-9
92234.81c     -2.33e-3
92235.81c     -0.13891
92236.81c     -4.1224e-3
92238.81c     -0.750811
 8016.81c     -0.086060
 6000.81c     -0.017760

% --- coolant: Helium, assuming all he-4

mat cool      -0.00408 rgb 50 100 100
 2004.81c     1.00


mat graph    -1.74 rgb 100 100 100 moder grph 6000
 6000.81c    9.99E-01
 5010.81c    4.975E-07
 5011.81c    2.0025E-06

% --- graph mat can be used for carbon buffer in TRISO particle
% --- below input other TRISO mats

mat pyroc   -2.2 rgb 50 50 150 moder grph 6000
 6000.81c   1.0

mat sic     -3.1 rgb 50 150 50
14028.81c   0.4611
14029.81c   0.02343
14030.81c   0.01546
 6000.81c   0.5

therm grph grph.15t

 /************************
  * Geometry definitions *
  ************************/
%  --- current issue: particle distribution file created assuming
% --- particles are in the entire 3cm radii, but in actuality, the outer .5cm
% --- of the sphere has no triso particles in it

particle 1

fuel        0.02125
graph       0.03075
pyroc       0.03475
sic         0.03825
pyroc       0.04225
void

% --- based on HGTR tutorial from serpent wiki

surf 10 inf

cell 10 2 graph -10

pbed 10 2 "prtcls.inp"
% --- describing pebble: graphite

surf 1 sph  0.0 0.0 0.0 2.5
surf 2 sph  0.0 0.0 0.0 3.0
surf 3 cube 0.0 0.0 0.0 3.0

cell 50 0 fill 10  -1
cell 60 0 graph     1 -2
cell 70 0 cool      2 -3
cell 80 0 outside   3

set powdens 0.11 fuel
set fpcut 1E-10


dep daytot

